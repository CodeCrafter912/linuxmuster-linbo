# group specific grub.cfg template for linbo net boot, other os
# thomas@linuxmuster.net
# 20.07.2015
#

# default #@@nr@@
menuentry '@@osname@@' {

 set root="@@osroot@@"

 if [ -e /@@kernel@@ ]; then
  linux /@@kernel@@ root=@@partition@@ @@append@@
 elif [ -s /boot/grub/grub.cfg ] ; then
  configfile /boot/grub/grub.cfg
 elif [ -s /bootmgr ] ; then
  ntldr /bootmgr
 elif [ -s /grldr ] ; then
  ntldr /grldr
 elif [ -s /ntldr ] ; then
  ntldr /ntldr
 else
  chainloader +1
 fi

}

